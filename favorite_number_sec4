//store and print favorite_number 
#include <iostream>
int main(){
    int favorite_number;
    std::cout << "Pick a number between 1-1000: ";
    std::cin >> favorite_number;
    std::cout << "Wow that's My Favorite Number." << std::endl;
    std::cout << "Im serious," << favorite_number <<" is my favorite!" << std::endl;
    return 0;    
}
